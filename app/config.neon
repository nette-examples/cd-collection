common:
	php:
		date.timezone: Europe/Prague

	services:
		database:
			class: Nette\Database\Connection ("sqlite2:%appDir%/models/demo.db")

		model:
			class: Model

		authenticator:
			factory: @model::createAuthenticatorService


production < common:

development < common:
